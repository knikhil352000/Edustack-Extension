{
  "manifest_version": 2,
  "name": "Edustack Extension",
  "author": "Edustack",
  "version": "1.0.0",
  "description": "Add the url to the Edustack Homework",
  "icons": {
    "128": "edustack_128.png"
  },
  "browser_action": {
    "default_popup": "index.html",
    "default_title": "Open Edustack"
  },
  "permissions": ["tabs", "activeTab", "<all_urls>", "storage"]
}
